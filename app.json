{
  "expo": {
    "name": "FuryuuDesigners",
    "slug": "furyuu",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_new.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/logo_new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSContactsUsageDescription": "Allow $(PRODUCT_NAME) to access your contacts."
      }
    },
    "android": {
      "icon": "./assets/logo_new.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_new.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_CONTACTS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.furyuu.app",
      "googleServicesFile": "./google-services.json",
      "manifestPath": "android/app/src/main/AndroidManifest.xml"
    },
    "web": {
      "favicon": "./assets/logo_new.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "useLegacyPackaging": true,
            "minSdkVersion": 24,
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "kotlinVersion": "1.9.24"
          }
        }
      ],
      [
        "expo-share-intent",
        {
          "androidIntentFilters": [
            "text/*",
            "image/*"
          ],
          "androidMultiIntentFilters": [
            "image/*"
          ]
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logo_new.png",
          "color": "#ffffff",
          "mode": "default",
          "androidMode": "default"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them.",
          "cameraPermission": "The app accesses your camera to let you take photos."
        }
      ],
      [
        "@react-native-google-signin/google-signin"
      ],
      "react-native-compressor"
    ],
    "scheme": "myapptailor",
    "owner": "thaiyal",
    "extra": {
      "eas": {
        "projectId": "0f689d43-2f1f-4a99-8eb4-52f8f104cb46"
      }
    }
  }
}
